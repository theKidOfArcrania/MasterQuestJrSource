#new:Script $Script_Interact
{
	Call     SpeakToPlayer 	( .Npc:Self 00A20009 00A20002 00000000 $TauntString )
	Call     ShowChoice		( 001E001D ) % yes / no
	If	*Var[0]  ==  0
		Call     ContinueSpeech	( .Npc:Self 00A20009 00A20002 00000000 $AcceptString )
		Call     DisablePlayerInput	( .True )
