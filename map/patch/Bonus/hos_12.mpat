#define .MasterFight *GameByte[0CE]
	
#new:Script $Script_Defeat
{
   	Call     SetMusicTrack		( 00000000 .Song:ShootingStarSummit 00000000 00000008 )
	Set  *MapFlag[0] .False
	Call     SetPlayerAnimation 	( 00010002 )
	Call     GetBattleOutcome 	( *Var[0] )
	Switch  *Var[0] 
	Case  ==  00000001 
		Call     SpeakToPlayer 	( 00000003 00A20009 00A20002 00000000 $YouGotRekt ) % Hm... You lack strength. You have skill, though. P ...
	Case  ==  00000002 
		Call     SpeakToPlayer 	( 00000003 00A20009 00A20002 00000000 $YouGotRekt ) % Hm... You lack strength. You have skill, though. P ...
	Case  ==  00000000 
			If .MasterFight == 0`
				Call     SpeakToPlayer		( .Npc:Self 00A20009 00A20002 00000000 $DefeatStringFirst )
				Set .MasterFight 2`
				Set  *Var[A]  .Item:EarthquakeJump %Sorry it took so long Brotenko.
				Set  *Var[B]  00000002 
				Set  *Var[0]  *Var[A] 
				Call     ShowGotItem 	( *Var[0] 00000000 00000000 )
				Call     AddBadge    	( *Var[A] *Var[0] )
			Else
				If .MasterFight == 1`
					Call     SpeakToPlayer		( .Npc:Self 00A20009 00A20002 00000000 $DefeatStringFirst )
					Set .MasterFight 2`
					Set  *Var[A]  .Item:EarthquakeJump %Yay its the right badge!
					Set  *Var[B]  00000002 
					Set  *Var[0]  *Var[A] 
					Call     ShowGotItem 	( *Var[0] 00000000 00000000 )
					Call     AddBadge    	( *Var[A] *Var[0] )
				Else
					Call     SpeakToPlayer		( .Npc:Self 00A20009 00A20002 00000000 $DefeatString )
				EndIf
			EndIf
	EndSwitch
	Call     SetNpcAnimation 	( .Npc:Self 00A20002 )
	Call     DisablePlayerInput	( .False )
	Call     EnablePartnerAI 	( )	
	Return
	End	
}

