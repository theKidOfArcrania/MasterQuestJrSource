#new:Function $Ultra_Hammer
{
PUSH t0, RA
ADDI t0, r0, 2
SAB t0, 8010F291
POP t0, RA
JR RA
NOP
}

#new:Function $Shooting_Stars
{
PUSH t0, t1, ra
LIO t0, 8010F456
ORI t1, r0, 0083
SH  t1, 0000 (t0)
SH  t1, FFFE (t0)
SH  t1, FFFC (t0)
SH  t1, FFFA (t0)
SH  t1, FFF8 (t0)
SH  t1, FFF6 (t0)
SH  t1, FFF4 (t0)
SH  t1, FFF2 (t0)
SH  t1, FFF0 (t0)
SH  t1, FFEE (t0)
POP t0, t1, ra
JR RA
NOP
}

