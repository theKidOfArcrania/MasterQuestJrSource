#new:Function $GlobalInRoom
{
PUSH v1, a0, a1, a2, a3, ra
LAW      a0, $AFlag
BEQ      a0, r0, .end
NOP
LIO 	   a0, $ParasolActive
ADDIU    a1, R0, 000A
DADDU    a2, R0, R0
JAL 	802C33E0 %Start Script
NOP
.end
POP v1, a0, a1, a2, a3, ra
JR RA
NOP
}

